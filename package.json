{
  "name": "ol-esri-style",
  "version": "4.1.0",
  "description": "Convert ESRI styles to OpenLayers styles",
  "main": "src/index.js",
  "type": "module",
  "targets": {
    "main": {
      "source": "src/index.js",
      "outputFormat": "esmodule",
      "isLibrary": true
    },
    "browser": {
      "source": "example/index.html",
      "context": "browser"
    }
  },
  "scripts": {
    "dev": "parcel example/index.html",
    "test": "mocha --require ./tests/setupTests.js --require jsdom-global/register ./tests/**/*.test.js",
    "test-watch": "mocha --watch --require @babel/polyfill --require @babel/register ./tests/**/*.test.js -r esm",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bojko108/ol-esri-style.git"
  },
  "keywords": [
    "openlayers",
    "esri",
    "styles"
  ],
  "author": "bojko108 <bojko108@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bojko108/ol-esri-style/issues"
  },
  "homepage": "https://github.com/bojko108/ol-esri-style#readme",
  "dependencies": {
    "ol": "^10.6.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.27.1",
    "buffer": "^6.0.3",
    "chai": "^5.2.1",
    "chai-as-promised": "^8.0.1",
    "esm": "^3.2.25",
    "jsdom": "26.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^11.7.1",
    "parcel": "^2.15.4",
    "prettier": "^3.7.4"
  }
}
