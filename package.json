  },
  "homepage": "https://github.com/bojko108/ol-esri-style#readme",
  "dependencies": {
    "ol": "^6.14.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.12.13",
    "chai": "^4.3.0",
